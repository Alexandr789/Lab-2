using System;

namespace Laboratorka2
{
    class Address

    {

        public string index; 

        public string strana;     

        public string gorod;     

        public string ylitsa;    

        public int nomerylitsu;     

        public int nomerkvartiru;     

        public void GetInfo()

        {

            Console.WriteLine($"Индекс: {index} \nСтрана: {strana} \nГород: {gorod} \nНазвание Улицы: {ylitsa} \nНомер Улицы: {nomerylitsu} \nНомер Квартиры:: {nomerkvartiru}");

        }

    }

    class Program

    {

        static void Main(string[] args)

        {

            Address home = new Address();

            home.index = "09100";

            home.strana = "Ukraine";

            home.gorod = "Bila Tserkva";

            home.ylitsa = "Dekabrustiv";

            home.nomerylitsu = 44;

            home.nomerkvartiru = 2;

            home.GetInfo();

            Console.ReadKey();

        }

    }
}
