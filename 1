using System;

namespace Lab 2-1
{
    class Address

    {

        public string index; 

        public string strana;     

        public string gorod;     

        public string ylitsa;    

        public int nomerylitsu;     

        public int nomerkvartiru;     

        public void GetInfo()

        {

            Console.WriteLine($"index: {index} \ncountry: {strana} \ncity: {gorod} \nstreet: {ylitsa} \nhouse: {nomerylitsu} \napartment: {nomerkvartiru}");

        }

    }

    class Program

    {

        static void Main(string[] args)

        {

            Address home = new Address();

            home.index = "09100";

            home.strana = "Ukraine";

            home.gorod = "Bila Tserkva";

            home.ylitsa = "Dekabrustiv";

            home.nomerylitsu = 44;

            home.nomerkvartiru = 2;

            home.GetInfo();

            Console.ReadKey();

        }

    }
}
